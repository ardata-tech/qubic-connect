{
  "version": "0.1.0",
  "description": "Qubic Connect is a MetaMask extension that has Qubic capabilities such as public key derivation and transaction signing.",
  "proposedName": "Qubic Connect",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "ch6Ux0sLzJRxMSabnCJnS9+5eCtA4z/MNy5+f3ILCpM=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_getBip32Entropy": [
      {
        "path": ["m", "44'", "3'"],
        "curve": "secp256k1"
      }
    ],
    "snap_getBip32PublicKey": [
      {
        "path": ["m", "44'", "3'", "0'", "0", "0"],
        "curve": "secp256k1"
      }
    ],
    "snap_getBip44Entropy": [
      {
        "coinType": 83293
      }
    ],
    "snap_getEntropy": {},
    "snap_dialog": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    }
  },
  "platformVersion": "6.14.0",
  "manifestVersion": "0.1"
}
